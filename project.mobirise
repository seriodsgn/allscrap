          "_name": "menu1",
          "_customHTML": "<section class=\"menu horizontal-menu\" group=\"Menu\" plugins=\"DropDown, amp-main,amp-sidebar\" global once=\"menu\">\n\n    <mbr-parameters>\n        <!-- Block parameters controls (Blue \"Gear\" panel) -->\n        <header>Show/Hide</header>\n        <input type=\"checkbox\" title=\"Show Logo\" name=\"showLogo\" checked>\n        <input type=\"checkbox\" title=\"Show Contacts\" name=\"showContacts\" checked>\n        <input type=\"range\" inline title=\"Icons Count\" name=\"iconsCount\" min=\"1\" max=\"5\" value=\"1\" step=\"1\">\n        <input type=\"checkbox\" title=\"Show Brand Name\" name=\"showBrand\" checked>\n        <input type=\"checkbox\" title=\"Show Button(s)\" name=\"showButtons\">\n        <input type=\"checkbox\" title=\"Sticky\" name=\"sticky\" checked>\n        <select title=\"Sidebar Side\" name=\"sidebarSide\">\n            <option value=\"left\">Left</option>\n            <option value=\"right\" selected>Right</option>\n        </select>\n        <header>Colors</header>\n        <input type=\"color\" title=\"Title BG Color\" name=\"menuBgColor3\" value=\"#ffffff\">\n        <input type=\"color\" name=\"icons\" title=\"Icons Color\" value=\"#ffffff\">\n        <input type=\"color\" name=\"ddcolor\" title=\"Drop Down BG Color\" value=\"#ffffff\">\n        <input type=\"color\" name=\"ddcolorlink\" title=\"Drop Down Link Color\" value=\"#232323\">\n        <!-- End block parameters -->\n    </mbr-parameters>\n    <nav class=\"navbar navbar-dropdown navbar-expand-lg\" mbr-class=\"{'navbar-fixed-top':sticky}\">\n        <div class=\"top-menu\">\n            <div class=\"top-menu-wrapper\">\n                <div class=\"top-box\">\n\n                    <div class=\"brand\" mbr-if=\"showLogo || showBrand\">\n                        <span mbr-if=\"showLogo\" class=\"brand-logo\">\n                            <amp-img src=\"@PROJECT_PATH@/assets/images/mbr-120x120.png\" layout=\"responsive\" width=\"60\" height=\"60\" alt=\